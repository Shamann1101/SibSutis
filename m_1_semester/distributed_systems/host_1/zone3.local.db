;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.zone3.local. root.zone3.local. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns1.zone3.local.
ns1     IN      A       192.168.0.10
host-1	IN      CNAME	ns1
host-2  IN      A       192.168.0.20
