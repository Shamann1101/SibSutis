;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	pool.ntp.local. root.ntp.local. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	pool.ntp.local.
pool    IN      A       192.168.0.10
pool    IN      A       192.168.0.20
